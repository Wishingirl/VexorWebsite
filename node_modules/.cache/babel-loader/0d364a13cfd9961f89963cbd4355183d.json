IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,WAAA,CAAA,KAAA;AACA,OAFA;AAGA,KAhBA;AAiBA,IAAA,eAjBA,2BAiBA,KAjBA,EAiBA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,IAAA;AACA,KAnBA;AAoBA,IAAA,cApBA,0BAoBA,KApBA,EAoBA;AACA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,UAAA,CAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA;;AACA,UAAA,CAAA,QAAA,IAAA,CAAA,CAAA,CAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,EAAA,CAAA;AACA;AACA;AA3BA,GAlBA;AA+CA,EAAA,IA/CA,kBA+CA;AACA,WAAA;AACA,MAAA,UAAA,EAAA;AADA,KAAA;AAGA;AAnDA,CAAA","sourcesContent":["<template>\n  <div :class=\"classObject\" v-if=\"edited\">\n    <fish-tag :key=\"item\" :index=\"item\" v-for=\"(item, index) in items\"\n            @click=\"clickHandler($event, index)\" @close=\"closeHandler($event, index)\">\n      {{typeof item === 'object' ? item[1] : item}}\n    </fish-tag>\n    <a class=\"fish tag\" v-if=\"onAddExists && !isClickAdd\" @click=\"addHandler\">\n      <i class=\"fa fa-plus\" style=\"margin-left:0;\"></i>\n    </a>\n    <input type=\"text\"\n           :key=\"`new-tag-input`\"\n           ref=\"newTagInput\"\n           @keyup.enter=\"addEnterHandler($event)\"\n           @blur=\"addDoneHandler($event)\"\n           v-if=\"onAddExists && isClickAdd\"/>\n  </div>\n  <div :class=\"classObject\" v-else>\n    <a :class=\"['fish tag', typeof item === 'object' && item[0] && 'primary']\"\n       @click=\"clickHandler($event, index)\"\n       :key=\"index\" v-for=\"(item, index) in items\">\n      {{typeof item === 'object' ? item[1] : item}}\n    </a>\n  </div>\n</template>\n<script>\n  import fishTag from './Tag'\n  export default {\n    name: 'fish-tags',\n    components: {\n      fishTag\n    },\n    props: {\n      size: String,\n      edited: { type: Boolean, default: false },\n      items: { type: Array, required: true } //\n    },\n    computed: {\n      classObject () {\n        return ['fish tags', this.size]\n      },\n      onAddExists () {\n        return this._events.add\n      }\n    },\n    methods: {\n      clickHandler (event, index) {\n        if (typeof this.items[index] === 'object') {\n          this.$emit('click', index, this.items[index][0] === false)\n        } else {\n          this.$emit('click', index)\n        }\n      },\n      closeHandler (event, index) {\n        this.$emit('close', index)\n      },\n      addHandler () {\n        this.isClickAdd = true\n        this.$nextTick(() => {\n          this.$refs.newTagInput.focus()\n        })\n      },\n      addEnterHandler (event) {\n        this.$refs.newTagInput.blur()\n      },\n      addDoneHandler (event) {\n        // console.log(event.target)\n        this.isClickAdd = false\n        let v = event.target.value\n        if (!/^\\s*$/.test(v)) {\n          this.$emit('add', v)\n        }\n      }\n    },\n    data () {\n      return {\n        isClickAdd: false\n      }\n    }\n  }\n</script>\n\n"],"sourceRoot":"node_modules/fish-ui/src/components"}]}