,CAAA,KAAA,UAAA,GAAA,UAAA,GAAA,KAAA,KAAA,EAAA,KAAA,CAAA,GAAA,CAAA;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,GAAA,IAAA;AACA,WAAA,aAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,KAdA;AAeA,IAAA,YAfA,0BAeA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,WAAA,aAAA,CAAA,EAAA;AACA,KAlBA;AAmBA,IAAA,WAnBA,yBAmBA;AACA,WAAA,OAAA,GAAA,KAAA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,KAtBA;AAuBA,IAAA,aAvBA,yBAuBA,CAvBA,EAuBA;AACA,WAAA,KAAA,CAAA,OAAA,EAAA,CAAA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,CAAA;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,IAAA;AACA;AA3BA;AAnBA,CAAA","sourcesContent":["<template>\n  <div class=\"fish input time-picker\"\n       @click=\"clickHandler\"\n       @mouseover=\"mouseOverHandler\"\n       @mouseout=\"mouseOutHandler\"\n       v-clickoutside=\"awayHandler\">\n    <input type=\"text\" :placeholder=\"hint\" :value=\"value\" style=\"width: 125px;\" readonly/>\n    <i class=\"fa fa-times-circle\" style=\"opacity: .6;\" @click.stop=\"clearHandler\" v-if=\"showClear && !valueEmpty\"></i>\n    <i class=\"fa fa-calendar\" v-else></i>\n    <div v-if=\"visible\" class=\"content\" :style=\"{width: `${(62 + 2) * groups.length}px`}\">\n      <ul class=\"fish menu vertical\" v-for=\"(group, groupIndex) in groups\">\n        <li :class=\"['item', {'active': item === (value.split(':')[groupIndex] || '00')}]\"\n            @click.stop=\"selectHandler(item, groupIndex)\"\n            v-for=\"item in group\">{{ item }}</li>\n      </ul>\n    </div>\n  </div>\n</template>\n<script>\n  import clickoutside from '../directives/clickoutside'\n  import { calendar, notify } from '../config'\n  export default {\n    name: 'fish-time-picker',\n    directives: { clickoutside },\n    props: {\n      value: { type: String, default: '00:00:00' },\n      hint: { type: String, default: '' }\n    },\n    data () {\n      return {\n        showClear: false,\n        visible: false,\n        groups: [calendar.hours, calendar.minutes, calendar.seconds]\n      }\n    },\n    computed: {\n      valueEmpty () {\n        return /^\\s*$/.test(this.value)\n      }\n    },\n    methods: {\n      mouseOverHandler () {\n        this.showClear = true\n      },\n      mouseOutHandler () {\n        this.showClear = false\n      },\n      clickHandler () {\n        this.visible = !this.visible\n      },\n      selectHandler (item, index) {\n        let values = (this.valueEmpty ? '00:00:00' : this.value).split(':')\n        values[index] = item\n        this.changeHandler(values.join(':'))\n      },\n      clearHandler () {\n        this.visible = false\n        this.changeHandler('')\n      },\n      awayHandler () {\n        this.visible = false\n        this.showClear = false\n      },\n      changeHandler (v) {\n        this.$emit('input', v)\n        this.$emit('change', v)\n        notify.field.change(this)\n      }\n    }\n  }\n</script>\n"],"sourceRoot":"node_modules/fish-ui/src/components"}]}